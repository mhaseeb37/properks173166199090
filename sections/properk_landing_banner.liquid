<style>
   .devAnimatedPAth  svg line {
    stroke-dasharray: 1000; /* Large enough value to ensure the line is initially hidden */
    stroke-dashoffset: 1000;
    transition: stroke-dashoffset 2s ease-in-out;
  }
  .devAnimatedPAth svg{
    width:100%;
  }
  .devAnimatedPAth svg.scrolled line {
    stroke-dashoffset: 0;
  }
    .devAnimatedPAth{
      padding-top:35px;
    }
   #animated-line path {
      stroke-dasharray: 3000; /* Adjust based on path length */
      stroke-dashoffset: 3000; /* Initially hide the stroke */
      transition: stroke-dashoffset 2s ease-in-out;
    }
    .scrolled .gradient-stop {
      transition: offset 2s ease-in-out;
    }

    .scrolled .gradient-stop {
      offset: 1; /* Moves the gradient stops to the end */
    }

    /* When scrolled into view, this class will trigger the path animation */
    #animated-line.scrolled path {
      stroke-dashoffset: 0;
    }
</style>
<script>
  document.addEventListener('DOMContentLoaded', function() {
    const svgElement = document.getElementById('animated-line');

    const observer = new IntersectionObserver((entries, observer) => {
      entries.forEach(entry => {
        if (entry.isIntersecting) {
          svgElement.classList.add('scrolled');
          observer.unobserve(svgElement);
        }
      });
    }, {
      root: null,
      threshold: 0.6
    });

    observer.observe(svgElement);
  });
</script>

