<script>
  document.addEventListener("DOMContentLoaded", function() {
    const perksQualifiedPurchases = parseInt("{{ customer.metafields.klaviyo.ProPerks_Qualified_Purchases | default: 0 }}", 10);
    const properksStatus = "{{ customer.metafields.klaviyo.properks_status | default: '' }}";

    if (perksQualifiedPurchases >= 1) {
      document.querySelector('.first_achievement').classList.add('active');
    }

    if (perksQualifiedPurchases >= 2) {
      document.querySelector('.second_achievement').classList.add('active');
    }

    if (properksStatus === "Expired") {
      document.querySelector('.third-achievement').classList.add('expired');
    } else {
      if (perksQualifiedPurchases === 1 || perksQualifiedPurchases === 2) {
        document.querySelector('.third-achievement').classList.add('yellow');
      }

      if (perksQualifiedPurchases === 3) {
        document.querySelector('.third-achievement').classList.add('active');
      }
    }
  });
</script>