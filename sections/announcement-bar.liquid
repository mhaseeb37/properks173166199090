    }
    .properks_inner_m {
    overflow-y: auto;
}
    .main_ul_properks{
      padding:0 !important;
    }
 .main_ul_properks li {
    border-radius: 50px;
    border: 2px solid #343333;
    display: flex;
    justify-content: center;
    align-items: center;
    width: fit-content;
    padding: 10px 20px;
    color: white;
    font-size: 13px;
}
