{%- liquid
  assign blocks = section.blocks
  assign options = section.settings

  assign color_scheme = options.color_scheme
  assign show_navigation_arrows = options.show_navigation_arrows
  assign show_social_list = options.show_social_list
  assign show_language_selector = options.show_language_selector
  assign show_currency_selector = options.show_currency_selector
  assign show_only_on_homepage = options.show_only_on_homepage

  assign container_size = options.container_size

  assign autoplay_mode = options.autoplay_mode
  assign autoplay_interval = options.autoplay_interval

  assign show_announcement_bar = true
  if show_only_on_homepage == true and request.page_type != 'index'
    assign show_announcement_bar = false
  endif

  assign add_side_paddings = options.add_side_paddings
  assign desktop_spacing_top = options.desktop_spacing_top
  assign desktop_spacing_bottom = options.desktop_spacing_bottom
  assign mobile_spacing_top = options.mobile_spacing_top
  assign mobile_spacing_bottom = options.mobile_spacing_bottom
-%}
<style>
  .pro-perks-row:hover p{
    text-decoration:underline;
  }
  .carousel__btn.announcement-bar__carousel-btn.btnarrow-left{
    right:0px;
   
    position:relative;
     transition:all 0.3s ease;
  }
  .announcement-bar__text{
    position:relative;
    left:0px;
    transition:all 0.3s ease-in-out;
    
  }
  .announcement-bar__text:hover{
    position:relative;
    left:5px;
    
  }
  .carousel.announcement-bar__carousel{
    width: 58%;
  }
  .caption.image-banner__caption {
    font-family: Inter, sans-serif;
    font-size: 13px;
    font-style: normal;
    font-weight: 500;
    line-height: 18px;
}
  .carousel__btn.announcement-bar__carousel-btn.btnarrowright{
    left:0px;
     position:relative;
      transition:all 0.3s ease;
  }
  .carousel__btn.announcement-bar__carousel-btn.btnarrow-left:hover{
    right:5px;
   
    position:relative;
     transition:all 0.3s ease;
  }
  .carousel__btn.announcement-bar__carousel-btn.btnarrowright:hover{
    left:5px;
     position:relative;
      transition:all 0.3s ease;
  }
  .shopify-section-announcement-bar {
    z-index:20;
  }
  .properks_inner_m {
    height: 100vh;
   overflow:auto;
    position: fixed;
    left: 0px;
    width: 100%;
    background: black;
    top: -1000%!important;
    transition:all 0.3s ease;
}
  .properks_inner_m.active{
     top:0!important;
}
.announcement-bar__text {
  cursor:pointer;
}
.header_btn_main {
  cursor:pointer;
}
  .search_main_d {
    max-width: 600px;
    margin: 0px auto;
    padding: 20px;
}
  .search_main_d .search-field{
    border-bottom:0px;
    padding:0px;
  }
  
  .search-field .input__search::placeholder{
      color:#00000096;
    }
 .poperks_body_inner .search_btn_properks {
    background:blue !important;
    color:white;
  }
 .poperks_body_inner .search_btn_properks .announcement-bar__chevron{
    transition:all 0.3s ease;
    position:relative;
   left:0px;
  }
  .poperks_body_inner .search_btn_properks:hover .announcement-bar__chevron{
    position:relative;
    left:4px;

     transition:all 0.3s ease;
  }
 .poperks_body_inner  .field .btn.btn--solid{
    background:blue !important;
    color:white;
  }
  .announcement-bar .field input {
    background: #bbbab82e!important;
}
  .input__search::placeholder{
     color:#00000096;
  }
  .input__search{
    color:black;
  }
  .field.properksfields{
    position:relative;
  }
  .main_search_heading {
    position: absolute;
    top: 16px;
    left: 10px;
}
  .input.input__search{
    padding-left:42px;
  }
  .search_btn_properks:hover{
    background:black!important;
    color:white!important;
  }
  ul.main_ul_properks {
    list-style: none;
    display: flex;
    padding: 0px 20px;
    flex-wrap:wrap;
    gap: 10px 15px;
}
  .search_suggestions_menu {
    max-width: 600px;
    margin: 0px auto;
    padding: 0px 20px;
    
}
  .main_ul_properks li{
