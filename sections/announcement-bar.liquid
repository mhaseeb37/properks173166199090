    gap: 5px;
}
    .main_ul_properks li a{
      text-decoration: none;
    } 
    .devContentCustom {
      /* padding-bottom:25px; */
    }
.devRelatedSingle {
    width: 100%;
    border-bottom: 1px solid #C7C7C7;
}
    .accordion-content {
    max-height: 0;
    overflow: hidden;
    transition: max-height 0.3s ease-out;
    display: flex;
    flex-direction: column;
    gap: 15px;
}
    .accordion-content a{
      padding-bottom:20px;
    }
  .accordion-header {
    cursor: pointer;
    padding: 10px;
  }
.devBlockTitleAnnounce{
  position:relative;
  transition: transform 0.3s ease;
}
    .devBlockTitleAnnounce svg{
      display:block;
    }
  .accordion-header .bi-chevron-down  {
    position:absolute;
    right:10px;
  }
    .accordion-header.active .bi-chevron-down {
     transform: rotate(180deg); 
  }
  /* .accordion-header.active::after {
    content: '-';
  } */

  .accordion-header.active + .accordion-content {
    max-height: 500px; /* Adjust this height according to the content size */
  }
    .devScrossText{
      display:block;
    }
    .header_btn_main svg{
      display:none;
    }
     .poperks_body_inner  .field .btn.btn--solid svg path{
        fill:white !important;
      }
    
}

</style>
{%- capture css -%}
  {% style %}
    .shopify-section-announcement-bar {
      {% if add_side_paddings %}
        --gsc-padding-inline: {{settings.aside_padding_size}}px;
      {% else %}
        --gsc-padding-inline: 0px;
      {% endif %}

      --gsc-desktop-spacing-top: {{desktop_spacing_top}}px;
      --gsc-desktop-spacing-bottom: {{desktop_spacing_bottom}}px;

      --gsc-mobile-spacing-top: {{mobile_spacing_top}}px;
      --gsc-mobile-spacing-bottom: {{mobile_spacing_bottom}}px;
    }
    {%- if section.settings.enable_style -%}
     .announcement-bar__carousel-btn .announcement-bar__chevron{
        width: 14px;
        height: 14px;
      }
      .announcement-bar__carousel-btn .announcement-bar__chevron path{
        fill: #fff;
      }
      
    {%- endif -%}
    {%- if section.settings.enable_pro_perks -%}
        a.pro-perks-link{
          display: inline-block;
          text-decoration: none;
        }
        .pro-perks-row{
          display: flex;
          align-items: center;
          column-gap: 10px;
        }
        .pro-perks-row img{
          width: 25px;
          height: 25px;
        }
        .pro-perks-row p{
       font-family: "IBM Plex Mono", serif;
        font-style: normal;
        font-weight: 700;
        font-size: 13px;
        line-height: 24px;
        color: #FFF;
        margin: 0;
        padding: 0;
        }
        .announcement-bar__settings svg path{
          stroke: #fff;
        }
      .announcement-bar__settings svg{
        margin-left: 5px;
      }
   .header_properks{
     display:flex;
     justify-content:right;
     padding:40px;
   }
     
    
    {%- endif -%}
  {% endstyle %}
{%- endcapture -%}

{%- render 'inline-css-minifier', css: css -%}

{%- if show_announcement_bar == true -%}
  <div class='announcement-bar section section-{{ section.id }} color-{{ color_scheme }} container container--{{ container_size }}'>
    <div class='announcement-bar__outer-container container container--{{ container_size }}'>
      <div class='announcement-bar__inner-container '>
        <div class='announcement-bar__left-panel'>
          {%- if show_social_list == true -%}
            {%- render 'social-list', class: 'announcement-bar__social-list' -%}
          {%- endif -%}
          {%- if section.settings.enable_pro_perks -%}
            <a class="pro-perks-link" href="{{ section.settings.perks-link }}">
              <div class="pro-perks-row">
                <img src="{{ section.settings.icon-perk | img_url: 'master' }}">
                <p> {{ section.settings.perks-txt }}</p>
              </div>
            </a>
          {%- endif -%}
        </div>
        <div class='announcement-bar__center-panel'>
          {%- if blocks.size > 0 -%}
            <carousel-component
              class='carousel announcement-bar__carousel {% if container_size == 'full-screen' %}announcement-bar__carousel--extended{% else %}announcement-bar__carousel--base{% endif %}'
              {% if autoplay_mode == 'one_at_time' %}
                data-autoplay-mode='one_at_time'
                data-autoplay-interval='{{autoplay_interval}}'
                data-loop='infinite'
              {% endif %}
              data-section-id='{{ section.id }}'
              with-hover-pause
              data-draggable
              data-align='start'
              data-axis='x'
              data-contain-scroll='trimSnaps'
            >
              {%- if blocks.size > 1 and show_navigation_arrows == true -%}
                <carousel-button
                  class='carousel__btn announcement-bar__carousel-btn btnarrow-left'
                  data-direction='prev'
                  tabindex='0'
                >
                  {%- render 'icon',
                    icon_name: 'chevron',
                    class: 'announcement-bar__chevron x-flip',
                    stroke_width: 1
                  -%}
                </carousel-button>
              {%- endif -%}
              <div class='carousel__viewport embla' data-carousel-viewport>
                <div class='carousel__container announcement__carousel-container' data-carousel-container>
                  {%- for block in blocks -%}
                  {% case block.type %}
                     {% when 'announcement'  %}
                    <div
                      class='carousel__slide announcement-bar__slide block-{{ block.id }}'
                      block-id='{{ block.id }}'
                      {{ block.shopify_attributes }}
                    >
                      {%- if block.settings.link != blank -%}
                        <a
                          href='{{ block.settings.link.url }}'
                          class='announcement-bar__link'
                          aria-label='{{ block.settings.text | strip_html | escape }}'
                        ></a>
                      {%- endif -%}
                      <div class=' announcement-bar__text'>
                        {% assign custom_icon_image = block.settings.custom_icon_image %}
                        {% assign icon = block.settings.icon %}

                        {%- if custom_icon_image != blank -%}
                          <img
                            {% render 'image-attributes',
                              image: custom_icon_image,
                              size: 'small',
                              class: 'announcement-bar__text-icon'
                            %}
                          >
                        {%- elsif icon != 'none' -%}
                          {% render 'icon-pack', icon: icon, class: 'announcement-bar__text-icon' %}
                        {%- endif -%}

                        <div class='rte  announcement-bar__rte'>
                          {{ block.settings.text }}
                        </div>
                      </div>
                    </div>
                    {% endcase %}
                  {%- endfor -%}
                </div>
              </div>
              {%- if blocks.size > 1 and show_navigation_arrows == true -%}
                <carousel-button
                  class='carousel__btn announcement-bar__carousel-btn btnarrowright'
                  data-direction='next'
                  tabindex='0'
                >
                  {%- render 'icon', icon_name: 'chevron', class: 'announcement-bar__chevron', stroke_width: 1 -%}
                </carousel-button>
              {%- endif -%}
            </carousel-component>
          {%- endif -%}
        </div>
        <div class='announcement-bar__right-panel'>
          {%- if show_currency_selector == true or show_language_selector == true -%}
            <div class='announcement-bar__settings'>
              {%- if show_language_selector == true -%}
                <div class='localization-dropdown-wrapper'>
                  <dropdown-opener
                    class='localization-dropdown-toggle'
                    data-aria-expanded='false'
                    data-float-element-id='AnnouncementBarLanguageSelector'
                    data-trigger='click'
                    tabindex='0'
                  >
                    {{ localization.language.endonym_name | capitalize }}
                    {% render 'icon',
                      icon_name: 'normal_arrow',
                      class: 'localization-dropdown-toggle__chevron',
                      stroke_width: 1
                    %}
                  </dropdown-opener>
                  <float-element
                    class='localization-dropdown color-{{ color_scheme }}'
                    aria-hidden='true'
                    id='AnnouncementBarLanguageSelector'
                    data-placement='bottom-end'
                    data-main-axis-offset='{% if settings.round_level != "0" %} 8{% else %} 0{% endif %}'
                    data-strategy='fixed'
                    data-with-portal
                  >
                    <template data-float-element-template='AnnouncementBarLanguageSelector'>
                      <div class='localization-dropdown__body'>
                        <localization-selector>
                          {%- form 'localization', id: 'AnnouncementBarLanguageForm' -%}
                            {%- for language in localization.available_languages -%}
                              <button
                                class='localization-dropdown__item link-parent-for-hover {% if localization.language == language %} selected{% endif %}'
                                data-localization-item
                                data-value='{{ language.iso_code }}'
                                data-focus-trap-target
                                aria-label='{{ language.endonym_name | strip_html | escape }}'
                              >
                                <span class='underline-animation'>{{ language.endonym_name | capitalize }}</span>
                              </button>
                            {%- endfor -%}
                            <input
                              type='hidden'
                              name='locale_code'
                              data-localization-input
                              value='{{ localization.language.endonym_name }}'
                              id='AnnouncementBarLanguageForm-LocaleCodeInput'
                            >
                          {%- endform -%}
                        </localization-selector>
                      </div>
                    </template>
                  </float-element>
                </div>
              {%- endif -%}
              {%- if show_currency_selector == true -%}
                <div class='localization-dropdown-wrapper'>
                  <dropdown-opener
                    class='localization-dropdown-toggle'
                    data-aria-expanded='false'
                    data-float-element-id='AnnouncementBarCurrencySelector'
                    data-trigger='click'
                    tabindex='0'
                  >
                    {{ localization.country.currency.iso_code }}
                    {% render 'icon',
                      icon_name: 'normal_arrow',
                      class: 'localization-dropdown-toggle__chevron',
                      stroke_width: 1
                    %}
                  </dropdown-opener>
                  <float-element
                    class='localization-dropdown color-{{ color_scheme }}'
                    aria-hidden='true'
                    id='AnnouncementBarCurrencySelector'
                    data-placement='bottom-end'
                    data-main-axis-offset='{% if settings.round_level != "0" %} 8{% else %} 0{% endif %}'
                    data-strategy='fixed'
                    data-with-portal
                  >
                    <template data-float-element-template='AnnouncementBarCurrencySelector'>
                      <div class='localization-dropdown__body'>
                        <localization-selector>
                          {%- form 'localization', id: 'AnnouncementBarCurrencyForm' -%}
                            {%- for country in localization.available_countries -%}
                              <button
                                data-localization-item
                                class='localization-dropdown__item link-parent-for-hover {% if localization.country == country %} selected{% endif %}'
                                data-value='{{ country.iso_code }}'
                                data-focus-trap-target
                                aria-label='{{ country.name | strip_html | escape }} {{ country.currency.iso_code | strip_html | escape }}'
                              >
                                <span class='underline-animation'>
                                  {{- country.name }}, {{ country.currency.iso_code -}}
                                </span>
                              </button>
                            {%- endfor -%}
                            <input
                              type='hidden'
                              name='country_code'
                              data-localization-input
                              value='{{ localization.country.iso_code }}'
                              id='AnnouncementBarCurrencyForm-CountryCodeInput'
                            >
                          {%- endform -%}
                        </localization-selector>
                      </div>
                    </template>
                  </float-element>
                </div>
              {%- endif -%}
            </div>
          {%- endif -%}
        </div>
      </div>
    </div>
 <div class="properks_inner_m">
   <div class="header_properks">
     <div class="header_btn_main">
       <div class="devScrossText">Close</div>
<svg width="25" height="24" viewBox="0 0 25 24" fill="none" xmlns="http://www.w3.org/2000/svg">
<path d="M22.8813 2L2.47705 22.4043" stroke="white" stroke-width="3" stroke-linecap="round"/>
<path d="M22.8813 22.4043L2.47705 2" stroke="white" stroke-width="3" stroke-linecap="round"/>
</svg>
  </div>
     </div>
  <div class="poperks_body_inner">
    <div class="search_main_d">
     <form
      class='search__form'
      action='{{ routes.search_url }}'
      method='get'
      role='search'
      class='search'
      id='SearchPageForm'
    >
      <search-field class='search-field' data-sidebar-search-input-component data-with-load-focus>
    <div class="field properksfields">
         <div class="main_search_heading">
           <svg width="25" height="24" viewBox="0 0 25 24" fill="none" xmlns="http://www.w3.org/2000/svg">
<path d="M11.4114 18.2511C15.6917 18.2511 19.1615 14.7813 19.1615 10.501C19.1615 6.2208 15.6917 2.75098 11.4114 2.75098C7.1312 2.75098 3.66138 6.2208 3.66138 10.501C3.66138 14.7813 7.1312 18.2511 11.4114 18.2511Z" stroke="#6A6864" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
<path d="M17.1191 16.2012L21.9182 21.0003" stroke="#6A6864" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
</svg>

         </div>
          <input type=""  type='text'
          name='q'
          placeholder='Search'
          value='{{ search.terms | escape }}'
          data-sidebar-search-input
          data-search-field-input
          id='SearchPageForm-QueryInput-{{ section.id }}' class="input input__search input--large input--outline" value="" aria-required="true" autocorrect="off" autocapitalize="off"ired="" id="ContactFooter-ContactEmailInput-" style="background:#F6F6F6">
                      <button class="btn btn--solid btn--lg input-btn search_btn_properks" name="commit" type="submit" aria-label="Subscribe">
                        <!-- <svg class="newsletter__arrow" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg" aria-labelledby="title" role="img"> <title id="title">Arrow</title>        <path d="M2 12H20" stroke="currentColor" stroke-width="2"></path>        <path d="M15 7L20 12L15 17" stroke="currentColor" stroke-width="2"></path>      </svg> -->
                     {%- render 'icon', icon_name: 'chevron', class: 'announcement-bar__chevron', stroke_width: 1 -%}
    </button>
      
        </div>
  </search-field>
     </form>
  
    </div>
  <div class="main_search_suggetions">
    <div class="search_suggestions_menu">
      <ul class="main_ul_properks">
      {% assign properlinks = section.settings.innerlink %}
      {% for link in properlinks.links %}
       
        <li>
           <a href="{{ link.url }}">
          {{ link.title }}
           </a>
        </li>
        
      {% endfor %}
        </ul>
    </div>
  </div>
    <div class="devRelatedAnnoncePerk">
      {% for block in section.blocks %}
        {% case block.type %}
          {% when 'imagewithtext'  %}
              <div class="devRelatedSingle">
                {% if block.settings.devtitle != blank %}
                   <div class="devBlockTitleAnnounce accordion-header">
                    {% if block.settings.devImage != blank %} <img src="{{ block.settings.devImage | img_url:'master' }}"> {% endif %} {{ block.settings.devtitle }} 
                    <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-chevron-down" viewBox="0 0 16 16">
                      <path fill-rule="evenodd" d="M1.646 4.646a.5.5 0 0 1 .708 0L8 10.293l5.646-5.647a.5.5 0 0 1 .708.708l-6 6a.5.5 0 0 1-.708 0l-6-6a.5.5 0 0 1 0-.708"/>
                    </svg>
                   </div>
                {% endif %}
                <div class="devContentCustom accordion-content ">
                {% if block.settings.paragraph != blank %}
                   <div class="devBlockContentAnnounce">
                      <p>{{ block.settings.paragraph }}</p>
                   </div>
                {% endif %}
                {% if block.settings.url != blank %}
                  <a href="{{ block.settings.url }}">{{ block.settings.link-text }}</a>
                {% endif %}
                </div>
              </div>
          {% endcase %}
       {% endfor %}
        </div>
    </div>
   </div>
 </div>
  
{%- endif -%}

{% render 'save-section-data-in-global-scope', section: section %}
<script>
 var innernalc=document.querySelector(".announcement-bar__text");
  var body_header = document.querySelector("body");
  var cross_s=document.querySelector(".header_btn_main");
  var announcementmain=document.querySelector(".properks_inner_m");
  innernalc.addEventListener("click", function(){
    announcementmain.classList.toggle("active");
    body_header.style.overflow="hidden";
  })
  cross_s.addEventListener("click",function(){
    announcementmain.classList.toggle("active");
    body_header.style.overflow="auto";
  })


document.addEventListener('DOMContentLoaded', function() {
  const accordionHeaders = document.querySelectorAll('.accordion-header');

  accordionHeaders.forEach(header => {
    header.addEventListener('click', function() {
      // Toggle active class
      this.classList.toggle('active');

      // Toggle the max-height for the content
      const content = this.nextElementSibling;
      if (content.style.maxHeight) {
        content.style.maxHeight = null;
      } else {
        content.style.maxHeight = content.scrollHeight + 'px';
      }
    });
  });
});

