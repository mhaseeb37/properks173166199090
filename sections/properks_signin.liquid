<script>
  document.querySelector('.main_properks_signin #phone').addEventListener('input', function (e) {
    let input = e.target.value.replace(/\D/g, ''); // Remove non-digit characters

    if (input.length > 10) {
        input = input.substring(0, 10); // Limit to 10 digits
    }

    if (input.length > 3 && input.length <= 6) {
        input = input.replace(/(\d{3})(\d{1,3})/, '$1 $2');
    } else if (input.length > 6) {
        input = input.replace(/(\d{3})(\d{3})(\d{1,4})/, '$1 $2 $3');
    }

    e.target.value = input;
  });

    $("#date_picker").flatpickr({});
</script>

<script>
  var submit_btn=document.querySelector(".sign_upbtn");
   var formRegister=document.querySelector("#create_customer");
  var email_field=document.querySelector("#email");
  var phone_no=document.querySelector("#phone");
  var passwordSignout=document.querySelector("#password");
  var namecustomer=document.querySelector("#first_name");
  var dob=document.querySelector("#password");


  submit_btn.addEventListener("click", function(event){
      event.preventDefault();

     var email = email_field.value.trim();
     var phone = phone_no.value.trim();
     var password = passwordSignout.value.trim();
     var name = namecustomer.value.trim();
     var dateOfBirth = dob.value.trim();


     var isValid = true;


    var emailPattern = /^[^\s@]+@[^\s@]+\.[^\s@]+$/;
     if (!emailPattern.test(email)) {
         var parentEle=email_field.closest(".input_field_wrap");
          parentEle.classList.add("error_field");
         isValid = false;
     }


     if (!/^\d{10}$/.test(phone)) {
        var parentEle=phone_no.closest(".input_field_wrap");
          parentEle.classList.add("error_field");
         isValid = false;
     }


     if (password.length < 6) {
        var parentEle=passwordSignout.closest(".input_field_wrap");
          parentEle.classList.add("error_field");
         isValid = false;
     }


     if (name === "") {
          var parentEle=namecustomer.closest(".input_field_wrap");
          parentEle.classList.add("error_field");
         isValid = false;
     }


     if (dateOfBirth === "") {
      var parentEle=dob.closest(".input_field_wrap");
          parentEle.classList.add("error_field");
         isValid = false;
     }


     if (isValid) {
        formRegister.submit();

     }
   })
</script>
