<style>
  .main_properks_signin{
    padding-top:{{section.settings.padding_top}}px;
    padding-bottom:{{section.settings.padding_bottom}}px;
  }
  .main-properks_inner-container{
      max-width: 1600px;
      margin: 0px auto;
  }
  .logo_main_signup {
      max-width: 81px;
       width:100%;
  }

  .sign_up_form_subheading a {
    text-decoration:none;
    position:relative;
  }
    .sign_up_form_subheading a:before{
      content:"";
  position:absolute;
  width:100%;
      left:0px;
      bottom:0px;
  border-bottom:1px solid black;
  }
    .sign_up_form_subheading a strong{
      transition:all 0.4s ease;
    }
  .sign_up_form_subheading a:hover strong{
    position:relative;
    padding-left:5px;
  }

  .main_inner-properks {
      display: flex;
  }
   .register_side {
      flex: 1;
      padding-right: 61px;
      border-right: 1px solid black;
     max-width:50%;

  }
    .login_side {
      flex: 1;
       max-width:50%;
  }
  .main_form_d {
    padding-left:170px;
  }
  .sign_up_form_heading h2{
    font-size:28px;
    font-weight:600;
  }
  .sign_up_form_subheading * {
      color: #7E7E7E;
  }
    .main_form_inner {
      margin-top: 23px;
  }

    .main_field_form {
      width: 100%;
      margin: 6px 0px;
      padding: 22px;
      border-radius: 16px;
      border: none;
      background: #f9f9f9;
      font-size:12px;
  }
    .login_inner {
      margin-top: 84px;
      padding-left: 100px;
  }
  .sign_in_heading h2{
    font-size:32px;
    font-weight:500;

  }
  .inner_signinForm {
      margin-top: 45px;
  }
    .login_inner {
      margin-top: 84px;
      padding-left: 65px;
      padding-right: 170px;
  }
    .help_links {
      display: flex;
      gap: 30px;
      font-size: 15px;
      color: #b1b1b1;
      margin-top: 13px;
      margin-left: 11px;
  }
  button.btn.btn--solid.sign_inbtn {
      margin-top: 20px;
      margin-left: 10px;
  }
  .sign_inbtn{
    min-width:150px;
  }
  .sign_inbtn:hover{
    border:1px solid black;
    background:white!important;
    color:black;
    cursor:pointer;
  }
    label.form_field_label {
      color: #0000008c;
  }
    .input_field_wrap {
      position: relative;
  }
    label.form_field_label {
      color: #0000008c;
      position: absolute;
      top:23px;
      left:23px;
      transition:all 0.3s ease;
  }
    .main_field_form:focus {
      border:1px solid #B1B1B1;
    }
  .main_field_form:focus + .form_field_label{
   font-size: 10px;
      top: 13px;
      transform: unset;
      left: 23px;


  }
    .sign_up_form_subheading {
      margin-top: 10px;
  }
    .register_side form{
      margin-top:30px;
    }
    .login_side form{
       margin-top:67px;
    }
    sup {
      top: -2px;
  }
  .sign_upbtn {
    margin-top:20px;
    min-width:150px;
  }
  .sign_upbtn:hover {
    border:1px solid black;
    background:white!important;
    color:black;
    cursor:pointer;
  }
  .flatpickr-calendar {
    background:white!important;
    overflow:hidden;
    border-radius:16px;
  }
    .flatpickr-months .flatpickr-prev-month, .flatpickr-months .flatpickr-next-month{
      top:18px;
    }
    .flatpickr-months {
      padding: 16px 0px;
      background: #f9f9f9;
      margin-bottom: 10px;
  }
    .flatpickr-day.selected{

      background: #0061ff;
      border:#0061ff;
  }
    .flatpickr-day.today{
       border:1px solid #0061ff;
          background: #0061ff3b;
    }
    .main_field_form:not(:placeholder-shown) + .form_field_label {
      font-size: 10px;
      top: 13px;
      transform: unset;
      left: 23px;
      display:none;

    }
  .main_field_form::placeholder{
    opacity:0;
  }
  .main_field_form:focus-visible{
    outline:none;
    box-shadow:none;
  }
  .error_message {
      display: flex;
      align-items: center;
      gap: 8px;
      font-size: 12px;
    display:none;
    margin-left:24px;
  }
  .input_field_wrap.error_field .error_message{
    display:flex;
  }

    .input_field_wrap.error_field  .main_field_form{
          border: 2px solid red;
      background: #ff000029;
    }
  .membert {
      text-align: center;
      margin-bottom: 65px;
    display:none;
