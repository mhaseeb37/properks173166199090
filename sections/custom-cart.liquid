<script>
  document.addEventListener("DOMContentLoaded", function(){
    var head_accordion=document.querySelectorAll(".cart_items");
       var allcloseBtns=document.querySelectorAll(".close_btn_m-acc");
    head_accordion.forEach(function(accordion){
      accordion.addEventListener("click", function(){
      accordion.classList.toggle("active");
        const nextSibling=accordion.nextElementSibling;
        if (nextSibling.style.maxHeight){
          nextSibling.style.maxHeight=null;
        }
        else{
          nextSibling.style.maxHeight=nextSibling.scrollHeight + 'px';
        }
      
      })
    })
     allcloseBtns.forEach(function(ele){
       ele.addEventListener("click", function(){
      var getParent=ele.closest(".cart-item");
       var innerD=getParent.querySelector(".cart_items");
         
         var accordiond=getParent.querySelector(".product_main_cart");
         
         accordiond.style.maxHeight=null;
         innerD.classList.remove("active");
    })
    })
  })
