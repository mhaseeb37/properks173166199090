      "step": 2,
      "default": 42,
      "unit": "t:sections.featured-navigation.font_size.unit",
      "label": "t:sections.featured-navigation.font_size.label"
    },
    {
      "type": "range",
      "id": "letter_spacing",
      "min": -2,
      "max": 8,
      "step": 0.1,
      "default": 0,
      "unit": "t:sections.featured-navigation.letter_spacing.unit",
      "label": "t:sections.featured-navigation.letter_spacing.label"
    },
    {
      "type": "range",
      "id": "spacing_between_messages",
      "min": 0,
      "max": 140,
      "step": 2,
      "default": 56,
      "unit": "t:sections.featured-navigation.spacing_between_messages.unit",
      "label": "t:sections.featured-navigation.spacing_between_messages.label"
    },
    {
