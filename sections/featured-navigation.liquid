      "min": 0,
      "max": 100,
      "step": 1,
      "default": 20,
      "unit": "t:sections.featured-navigation.top_and_bottom_spacing.unit",
      "label": "t:sections.featured-navigation.top_and_bottom_spacing.label"
    },
    {
      "type": "checkbox",
      "id": "show_separators",
      "default": false,
      "label": "t:sections.featured-navigation.show_separators.label"
    },
    {
      "type": "header",
      "content": "t:sections.featured-navigation.scrolling_header.label"
    },
    {
      "type": "select",
      "id": "scrolling_type",
      "label": "t:sections.featured-navigation.scrolling_type.label",
      "options": [
        {
          "value": "none",
          "label": "t:sections.featured-navigation.scrolling_type.options.option_1"
        },
