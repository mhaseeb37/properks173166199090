{%- liquid
  assign with_only_links = true
  assign is_first_link = true
  assign is_second_link = true

  if btn_text != blank and btn_style != 'link'
    assign with_only_links = false
    assign is_first_link = false
  endif

  if extra_btn_text != blank and extra_btn_style != 'link'
    assign with_only_links = false
    assign is_second_link = false
  endif

  if text != blank
    assign with_only_links = false
  endif
-%}

{%- if caption != blank or title != blank or text != blank or btn_text != blank or extra_btn_text != blank -%}
  <div
    data-aos="fade-up"
    class="image-banner__content-wrap  {% if properk_landing %} properk_landing_banner{% endif %}"
  >
    <div class="image-banner__content image-banner__content--desktop image-banner__content--with-{{ desktop_content_background }}-background">
      {% if logo != blank %}
        <img
          {% render 'image-attributes', image: logo, size: 'medium', class: 'devProperkLogo' %}
        >
      {% endif %}
      {%- if caption != blank -%}
        <div class="caption image-banner__caption {% if desktop_content_background == 'solid' %} desktop-body-color {% endif %}">
          {{ caption }}
        </div>
      {%- endif -%}
      <div class="expired">
        {%- if title != blank -%}
          {% if properk_landing %}
            {% assign customer_name = customer.first_name %}
            {% assign currentyear = 'now' | date: '%Y' %}
            <h2 class="fadin image-banner__title {{ heading_size }}">
              {{ title | replace: '[Name]', customer_name | replace: '[this year]', currentyear }}
            </h2>
          {% else %}
            <h2 class="fadintitle image-banner__title {{ heading_size }}">{{ title }}</h2>
          {% endif %}
        {%- endif -%}
        {%- if text != blank -%}
          <div class="fadindes rte {{ body_size }} {% if desktop_content_background == 'solid' and title != blank %} desktop-body-color {% endif %} image-banner__text body-font-weight-from-global-settings">
            {{ text }}
          </div>
        {%- endif -%}
      </div>
