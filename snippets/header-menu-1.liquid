    </div>
 <div class="swiper-button-next">
       {% render 'icon', icon_name: 'chevron', class: 'carousel-btn__icon y-flip' %}
 
 </div>
    <div class="swiper-button-prev">
      {% render 'icon', icon_name: 'chevron', class: 'carousel-btn__icon x-flip' %}
</div>
</div>
{% if block.settings.btm_heading %}
<div class="footer_menu_main">
 
  {{ block.settings.btm_heading }}
  <div class="footer_bottom_btns">
    <a href="{{ btn1url }}">
    <button class="btn btn--outline rich-text__btn _desktop_m desktop_btn1">
      {{ block.settings.btn1text }}
    </button>
    </a>
      <a href="{{ btn2url }}">
     <button class="btn btn--outline rich-text__btn _desktop_m desktop_btn2" > {{ block.settings.btn2text }}</button>
      </a>
  </div>
</div>
  {% endif %}

<script>
   var swipermain = new Swiper(".mega-menu__block--images", {
      slidesPerView: 1,
      spaceBetween: 10,
       enabled:false,
      pagination: {
        el: ".swiper-pagination",
        clickable: true,
      },
     navigation: {
        nextEl: ".swiper-button-next",
        prevEl: ".swiper-button-prev",
      },
      breakpoints: {
        640: {
          slidesPerView: 2,
          spaceBetween: 10,
           enabled:false,
        },
        768: {
          slidesPerView: 4,
          spaceBetween: 10,
        },
        1024: {
          slidesPerView: 5,
          spaceBetween: 10,
           enabled:true,
        },
      },
     
    });
</script>