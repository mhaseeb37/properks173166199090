{%- liquid
  assign options = block.settings

  assign show_item_1 = options.show_item_1
  assign show_item_2 = options.show_item_2
  assign show_item_3 = options.show_item_3
  assign show_item_4 = options.show_item_4
  assign show_item_5 = options.show_item_5
  assign show_item_6 = options.show_item_6
  assign show_item_7 = options.show_item_7
  assign show_item_8 = options.show_item_8
  assign show_item_9 = options.show_item_9
    assign show_item_10 = options.show_item_10
    assign show_item_11 = options.show_item_11
    assign show_item_12 = options.show_item_12
    assign show_item_13 = options.show_item_13
     assign show_item_14 = options.show_item_14
  assign heading_size = options.heading_size
  assign body_size = options.body_size
  assign enable_vertical_text = options.enable_vertical_text
  assign content_position = options.content_position
  if content_position contains 'left'
    assign content_align = 'left'
  endif
  if content_position contains 'center'
    assign content_align = 'center'
  endif
  if content_position contains 'right'
    assign content_align = 'right'
  endif

  assign hover_effect = options.hover_effect
  assign block_visibility = options.block_visibility
-%}
<style>
.mega-menu__block--images .swiper-button-next::after{
  display:none;
}  
  .mega-menu__block--images .swiper-button-prev::after{
  display:none;
}  
 .mega-menu__block--images .swiper-button-next{
   height: 50px;
    width: 50px;
    background: white;
    border-radius: 50%;
    border: 2px solid black;
 }

  .mega-menu__block--images .swiper-button-prev{
   height: 50px;
    width: 50px;
    background: white;
    border-radius: 50%;
    border: 2px solid black;
 }
  .mega-menu__block--images .swiper-button-next svg{
   height: 30px;
    width: 30px;
    
 }
  .mega-menu__block--images .swiper-button-prev svg{
   height: 30px;
    width: 30px;
   
 }
.menu_header_main {
  padding:20px;
  text-align:center;
  font-weight:500;
}
  .footer_bottom_btns {
    text-align:center;
  }
  .footer_bottom_btns {
    display:flex;
    justify-content:center;
    gap:20px;
    
  }
  .footer_menu_main {
    padding:20px 0px;
    text-align:center;
  }
 .footer_bottom_btns {
  margin-top:20px;
}
.mega-menu__block .swiper-button-disabled {
  display:none;
}
  ._desktop_m.btn:hover{
    background:#000000;
    color:#ffffff;
    cursor:pointer;
  }
@media screen and (max-width: 640px){
  .mega-menu__block--images .swiper-wrapper{
    flex-wrap:wrap;
    gap:10px;
  }
   .footer_menu_main {
    padding: 20px 13px;
    text-align: center;
}
  .footer_bottom_btns{
flex-wrap: wrap;
  }
  .footer_bottom_btns .desktop_btn1:first-child{
    background:#323232; 
color:white;
  }
}
</style>
{% if block.settings.blkheading %}
<div class="menu_header_main">
  {{ block.settings.blkheading }}
</div>
  {% endif %}
<div
  class='{{ class }} mega-menu__block swiper mega-menu__block--images mega-menu__card-grid card-grid mega-menu-{{ block.id }} color-{{ block.settings.color_scheme }} mega-menu__block--spaced {% if block_visibility != 'desktop_and_mobile' %} mega-menu__block--only-{{block_visibility}}-visible {% endif %} {% if enable_vertical_text %} mega-menu__block--vertical mega-menu__block--{{content_align}} {% endif %}'
  block-id='{{ block.id }}'
  {{ block.shopify_attributes }}
>
    <div class="swiper-wrapper">
